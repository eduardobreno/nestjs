{
  "name": "nest-typescript-starter",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev:local": "APP_SERVER=local nest start --watch",
    "start:dev:stag": "APP_SERVER=staging nest start --watch",
    "start:dev:prod": "APP_SERVER=production nest start --watch",
    "start:debug": "APP_SERVER=local nest start --debug --watch",
    "start:stag": "APP_SERVER=staging node dist/main",
    "start:prod": "APP_SERVER=production node dist/main",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "echo 'No e2e tests implemented yet.'"
  },
  "dependencies": {
    "@nestjs/common": "7.2.0",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "7.2.0",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/mongoose": "7.0.1",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "7.2.0",
    "@nestjs/swagger": "^4.5.10",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "mongoose": "5.9.19",
    "nanoid": "^3.1.10",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "6.5.5",
    "swagger-ui-express": "^4.1.4"
  },
  "devDependencies": {
    "@nestjs/cli": "7.4.0",
    "@nestjs/schematics": "7.0.1",
    "@nestjs/testing": "7.2.0",
    "@types/express": "4.17.6",
    "@types/mongoose": "5.7.27",
    "@types/node": "14.0.13",
    "@types/supertest": "2.0.9",
    "@types/ws": "7.2.5",
    "@typescript-eslint/eslint-plugin": "3.3.0",
    "@typescript-eslint/parser": "3.3.0",
    "eslint": "7.3.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.21.2",
    "jest": "26.0.1",
    "prettier": "2.0.5",
    "supertest": "4.0.2",
    "ts-jest": "26.1.0",
    "ts-loader": "7.0.5",
    "ts-node": "8.10.2",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.9.5"
  }
}
